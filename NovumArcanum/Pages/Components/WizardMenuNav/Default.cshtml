@model NovumArcanum.Components.WizardMenuNav.ViewModel

@foreach (var wizard in Model.Wizards)
{
    if (wizard.IsDisplay || Model.HasSanctumPermission && Model.UserId == wizard.Id)
    {
        <li>
            <a class="nav-link" asp-page="/Wizard" asp-route-wizardId="@wizard.Id">@wizard.Name</a>
        </li>
    }
}